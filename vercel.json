{
  "version": 2,
  "builds": [
    {
      "src": "pro2/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "25mb",
        "runtime": "python3.11",
        "buildCommand": "pip install --upgrade pip && pip install -r requirements.txt"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "pro2/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles"
}
